    "if-env": "^1.0.4",
    "mongoose": "^5.9.15",
    "object-assign": "^4.1.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-iframe": "^1.8.0",
    "react-player": "^2.0.1",
